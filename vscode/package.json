{
  "name": "abysslang",
  "displayName": "AbyssLang",
  "description": "Official VSCode support for AbyssLang programming language.",
  "version": "0.1.0",
  "publisher": "abrorpatidinov",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbrorPatidinov/ProjectAbyss"
  },
  "homepage": "https://github.com/AbrorPatidinov/ProjectAbyss",
  "bugs": {
    "url": "https://github.com/AbrorPatidinov/ProjectAbyss/issues"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "abyss",
    "abysslang",
    "programming language",
    "compiler",
    "vm"
  ],
  "contributes": {
    "languages": [
      {
        "id": "abyss",
        "aliases": [
          "AbyssLang",
          "abyss"
        ],
        "extensions": [
          ".al"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "abyss",
        "scopeName": "source.abyss",
        "path": "./syntaxes/abyss.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "abyss",
        "path": "./snippets/abyss.json"
      }
    ]
  }
}
